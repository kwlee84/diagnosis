<?xml version="1.0" encoding="ISO-8859-1"?>
<workbook>
    <worksheet name="Sheet1">
    	<section startRow="0" endRow="0"></section>
        <loop startRow="1" endRow="1" items="employees" var="employee" varType="models.Employee">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">employee.companyId</mapping>
                <mapping row="1" col="1">employee.team</mapping>
                <mapping row="1" col="2">employee.name</mapping>
                <mapping row="1" col="3">employee.email</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"/>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>